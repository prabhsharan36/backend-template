{
  "name": "clinicspots-server-new",
  "version": "1.0.0",
  "description": "Clinicspots Backend",
  "main": "build/app.js",
  "scripts": {
    "clean": "rimraf build/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "NODE_ENV=production npm-run-all clean tsc",
    "build-dev": "NODE_ENV=development npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "dev:dist": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/app.ts",
    "start": "node ."
  },
  "license": "ISC",
  "dependencies": {
    "awilix": "^8.0.0",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "reflect-metadata": "^0.1.13",
    "tracer": "^1.1.5",
    "typeorm": "0.3.10"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.186",
    "@types/node": "^16.11.10",
    "@types/nodemailer": "^6.4.4",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "ts-node": "10.7.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "4.5.2"
  }
}
